# microservice specific routes

POST          /authenticate                                    uk.gov.hmrc.thirdpartydevelopersession.controllers.AuthenticationController.authenticate
POST          /authenticate-mfa                                uk.gov.hmrc.thirdpartydevelopersession.controllers.AuthenticationController.authenticateAccessCode

GET           /session/:id                                     uk.gov.hmrc.thirdpartydevelopersession.controllers.UserSessionController.fetch(id: uk.gov.hmrc.apiplatform.modules.tpd.session.domain.models.UserSessionId)
PUT           /session/:id/loggedInState/:state                uk.gov.hmrc.thirdpartydevelopersession.controllers.UserSessionController.updateLoggedInState(id: uk.gov.hmrc.apiplatform.modules.tpd.session.domain.models.UserSessionId, state: uk.gov.hmrc.apiplatform.modules.tpd.session.domain.models.LoggedInState)
DELETE        /session/:id                                     uk.gov.hmrc.thirdpartydevelopersession.controllers.UserSessionController.delete(id: uk.gov.hmrc.apiplatform.modules.tpd.session.domain.models.UserSessionId)

